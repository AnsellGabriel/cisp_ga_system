

<div class="card">
  <div class="card-header">
    <h1>Geo province</h1>
    <%= link_to "Delete", geo_province, class: "btn btn-danger btn-sm", method: :delete, data: { "turbo-method": :delete, turbo_confirm: "Are you sure?" } %>
  </div>
  <div class="card-body">
    <%= render @geo_province %>
  </div>
  <div class="card-footer">
    <%= link_to "Edit", edit_geo_province_path(@geo_province), class: "btn btn-secondary btn-sm" %> |
    <%= link_to "Back", geo_provinces_path, class: "btn btn-secondary btn-sm" %>
  </div>

</div>